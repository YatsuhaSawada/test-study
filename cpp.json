	"GoogleTestTemplate": {
		"prefix": "gtesttemplate",
		"body": [			
			"\/\/ Test テスト説明",
			"TEST($1, $2) {",
			"    \/\/ Arrange 事前条件の設定",
			"",
			"    \/\/ Action テスト実行",
			"",
			"    \/\/ Assert 事後条件と事前条件の確認",
			"}"
		],
		"description": "TEST(test_case_name, test_name in test_case_name)"
	},
	"GoogleTestFileTemplate": {
		"prefix": "gtestfiletemplate",
		"body": [			
			"/**",
			"* @file $TM_FILENAME",
			"* @brief ",
			"* @copyright Copyright [$CURRENT_YEAR] <Copyright Owner>",
			"*/",
			"#pragma once",
			"",
			"#include \"gtest/gtest.h\"",
			"",
			"",
			"\/\/ Test テスト説明",
			"TEST($1, $2) {",
			"    \/\/ Arrange 事前条件の設定",
			"",
			"    \/\/ Action テスト実行",
			"",
			"    \/\/ Assert 事後条件と事前条件の確認",
			"}",
			"}"
		],
		"description": "Google Test FIle Template"
	},
	"CppHFileTemplate": {
		"prefix": "cpphfiletemplate",
		"body": [
			"/**",
			"* @file $TM_FILENAME",
			"* @brief ",
			"* @copyright Copyright [$CURRENT_YEAR] <Copyright Owner>",
			"*/",
			"#pragma once",
			"",
			"",
			"/**",
			"* @class xxx",
			"* @brief クラスの概要を記載",
			"* @invariant クラスの不変条件を記載　参照:https://en.wikipedia.org/wiki/Class_invariant のinvariant部分",
			"*/",
			"class xxx {",
			" public:",
			"     xxx();",
			"     ~xxx();",
			"",
			"     /**",
			"     * @brief ",
			"     * @pre 事前条件",
			"     * @post 事後条件",
			"     */",
			"     func();",
			"};",
			""
		],
		"description": "Cpp H File TemplateS"
	},
	"MethodComment": {
		"prefix": "comment_m",
		"body": [
			"/**",
			"* @brief ",
			"* @pre 事前条件",
			"* @post 事後条件",
			"*/"
		],
		"description": " Comment"
	}
}